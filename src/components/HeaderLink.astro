---
export interface Props {
  href: string;
  target?: string;
}

const { href, target } = Astro.props;
---

<a
  href={href}
  target={target}
  class="text-sm font-medium text-gray-700 hover:text-primary-600 dark:text-gray-300 dark:hover:text-primary-400 transition-colors duration-200"
>
  <slot />
</a>
